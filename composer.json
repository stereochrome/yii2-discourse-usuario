{
	"name": "stereochrome/yii2-discourse-usuario",
	"description": "Discourse SSO module for use with yii2-usuario.",
	"keywords": ["yii2", "framework", "discourse", "sso", "usuario"],
	"homepage": "https://github.com/stereochrome/",
	"type": "yii2-extension",
	"license": "BSD-3-Clause",
	"support": {
		"issues": "https://github.com/stereochrome/yii2-discourse-usuario/issues",
		"source": "https://github.com/stereochrome/yii2-discourse-usuario"
	},
	"require": {
		"php": ">=5.7",
		"2amigos/yii2-usuario": "*",
    	"sammaye/yii2-discourse-sso": "~1.0"
	},
	"require-dev": {
	},
	"autoload": {
		"psr-4": {
			"Stereochrome\\Discourse\\": "./src/Discourse"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Stereochrome\\Discourse\\": "./src/Discourse"
		}
	},
    "extra": {
       	"bootstrap": "Stereochrome\\Discourse\\Bootstrap"
   	}
}
